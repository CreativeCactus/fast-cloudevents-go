test(){ ## Curl tests
	curl -v -X POST \
		-H "Content-Type: application/cloudevents+json" \
		-H "ce-type: com.example.someevent" \
		-H "ce-time: $(date '+%FT%T.%N%:z')" \
		-H "ce-id: $(uuidgen)" \
		-H "ce-source: a/b/" \
		-H "ce-specversion: 1.0" \
		-H "ce-myextension: 1234" \
		-d @<(echo '{"hello":"world"}') \
		http://localhost:9090/
}
"${@:-test}"
